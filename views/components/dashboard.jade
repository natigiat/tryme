.container.progect
      .row
        .col-sm-10
          h1 #{name}
        .col-sm-2
          a.pull-right(href='/users')
              img.img-circle.img-responsive.imgProfile(title='profile image', src='#{image}')
    
       
      .row
        .col-sm-3
          // left col
          ul.list-group
            li.list-group-item.text-muted Profile
            li.list-group-item.text-right
              span.pull-left
                strong Joined
              |  #{date}
            li.list-group-item.text-right
              span.pull-left
                strong Package
              div.pull-right
                  |  #{rolename} 
                  if (rolename != "Full")
                      a(href='/account/progect#update' class='label label-info') Update
            li.list-group-item.text-right
              span.pull-left
                strong Real name
              |  #{name} 
          .panel.panel-default
            .panel-heading
              | Website 
              i.fa.fa-link.fa-1x
            .panel-body   
              button.btn.btn-success-tem.col-sm-12.addW.colorWhite(type='button', data-toggle='modal', data-target='#myModal')
                | Add Website
              // Modal
              #myModal.modal.fade(tabindex='-1', role='dialog', aria-labelledby='myModalLabel')
                .modal-dialog(role='document')
                  .modal-content
                    .modal-header
                      button.close(type='button', data-dismiss='modal', aria-label='Close')
                        span(aria-hidden='true') ×
                      h4#myModalLabel.modal-title Modal title
                    .modal-body
                       .row
                        .col-md-12.col-sm-12.col-xs-12
                              form.form-inline(role="form" , data-toggle="validator")
                                .form-group
                                  label.sr-only(for='exampleInputEmail2') Site Name
                                  input#sitename.form-control(type='text', name='siteName', placeholder='Site name...')
                                .form-group
                                  label.sr-only(for='exampleInputPassword2') Site Url
                                  input#siteurl.form-control(type="url", placeholder='http://example.com')
                                button.btn.btn-info.addSite(type='submit', title='') Add site
             


          
          ul.list-group
            li.list-group-item.text-muted
              | Activity 
              i.fa.fa-dashboard.fa-1x
            li.list-group-item.text-right
              span.pull-left
                strong Sites
                  span.sitesNumber 
            li.list-group-item.text-right
              span.pull-left
                strong Likes
              |  13
            li.list-group-item.text-right
              span.pull-left
                strong Posts
              |  37
            li.list-group-item.text-right
              span.pull-left
                strong Followers
              |  78

        // /col-3
        .col-sm-9
          ul#myTab.nav.nav-tabs
            li.active
              a(href='#home', data-toggle='tab') Home
            li
              a(href='#messages', data-toggle='tab') Messages
            if (role != "3") 
              li
                a(href='#update', data-toggle='tab') Update
          .tab-content
            #home.tab-pane.active
              .table-responsive
                table.table.table-hover
                  thead
                    tr
                      th Site Added
                      th Website Name
                      th Website Url
                      th Visit
                      th Download 
                      th 
                  tbody#items

                    
                hr
                .row
                  .col-md-4.col-md-offset-4.text-center
                    ul#myPager.pagination
        
            // /tab-pane
            #messages.tab-pane
              h2
              ul.list-group
                li.list-group-item.text-muted Inbox
                li.list-group-item.text-right
                  a.pull-left(href='#') Welcome to be responsive
                  |  2.13.2016
                li.list-group-item.text-right
                  a.pull-left(href='#') Hi #{name} , Start template your website..
                  |  2.13.2016
         
            // /tab-pane
            if (role != "3") 
              #update.tab-pane
                br
                if (role == "1") 
