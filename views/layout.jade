    //- body
    
    nav.navbar-default.navbar.navbar-static-top
        .container
          div
            button.navbar-toggle.collapsed(type='button', data-toggle='collapse', data-target='#navbar', aria-expanded='false', aria-controls='navbar')
              span.sr-only Toggle navigation
              span.icon-bar
              span.icon-bar
              span.icon-bar
            a(href='/')
             img.Navbar-brand(src='/images/4444.png')
          #navbar.collapse.navbar-collapse
            ul.nav.navbar-nav
              li(class=(title ==
               'Account' ? 'active' : ''))
                a(href='/account/') My site
              li(class=(title == 'Examples' ? 'active' : ''))
                a(href='/examples') Examples
              li(class=(title == 'Prices' ? 'active' : ''))
                a(href='/prices') Prices
              li(class=(title == 'Support' ? 'active' : ''))
                a(href='/support') Support


            ul.nav.navbar-nav.navbar-right
                if (user)
                  .dropdown.userInfo
                    if (image)
                      img(src="#{image}" class="profileImage")
                    else
                      img(src='/images/profile.png' class="profileImage")
                     
                    a.dropdown-toggle(href="#", data-toggle="dropdown")
                      | Hi , #{name}
                      b.caret
                    ul.dropdown-menu(role="menu")
                      li.arrowUp
                        a(href='/account/setting') Account Settings
                      li
                        a(href='/users/logout') Billing & Payments
                      li
                        a(href='/users/logout') Logout
                else 
                    ul.nav.navbar-nav.reg
                     li 
                       a(href='/users/register') Sign up 
                     li
                       a(href='/users/login') Sign in

    
  .container
    != messages()
    != toasts

  block content
  

  .clear
  footer.footer
    #content-root-footer(role='global')
        .plans-and-pricing-ribbon
            Incredible drowrow Flexible pricing.  
            a(href='http://www.istockphoto.com/plans-and-pricing') Buy credits or subscribe today.       
      .clearfix
  
